{
  "name": "formidable-react-component-boilerplate",
  "version": "0.0.1",
  "description": "react component boilerplate",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/formidable-react-component-boilerplate.git"
  },
  "author": "Lauren Eastridge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/formidable-react-component-boilerplate/issues"
  },
  "homepage": "https://github.com/FormidableLabs/formidable-react-component-boilerplate",
  "scripts": {
    "TODO-postinstall": "echo 'FIX COMMANDS'",
    "postinstall": "npm run clean && npm run lib",

    "TODO-lib-dist": "echo 'FIX COMMANDS'",
    "clean": "rimraf lib",
    "lib": "babel --stage 0 src/ -d lib/",
    "dist": "npm run lint-dist && webpack && webpack --config=webpack.config.minified.js",

    "TODO-dev-hot": "echo 'UPDATE DOCS / COMMANDS'",
    "dev": "webpack-dev-server --config demo/webpack.config.js --no-info --content-base demo/",
    "hot": "webpack-dev-server --config demo/webpack.config.js --no-info --content-base demo/ --hot",
    "open-demo": "opener http://localhost:8080/webpack-dev-server/",
    "demo": "npm run lint && npm run dev & npm run open-demo",
    "lint-node": "eslint -c .eslintrc-node *.js demo/webpack.*.js",
    "lint-react": "eslint --ext .js,.jsx -c .eslintrc-react src demo/*.jsx",
    "lint": "npm run lint-node && npm run lint-react",

    "TODO-MISC": "echo '(1) prune dependencies, (2) dev vs. prod deps'",
    "test": "echo 'TODO (1) Client 11, (2) Server 13, (3) Integration 12 tests.'",
    "check": "npm run lint && npm run test"
  },
  "dependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "eslint": "^0.24.1",
    "rimraf": "^2.4.0",
    "webpack": "^1.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.0",
    "babel-loader": "^5.3.2",
    "css-loader": "~0.9.0",
    "eslint-config-defaults": "^3.0.3",
    "eslint-plugin-react": "^2.6.4",
    "lodash": "^3.9.3",
    "opener": "^1.4.1",
    "react": "0.13.x",
    "style-loader": "~0.8.0",
    "url-loader": "~0.5.5",
    "webpack-dev-server": "^1.10.0"
  }
}
