{
  "name": "formidable-react-component-boilerplate",
  "version": "0.0.1",
  "description": "react component boilerplate",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/formidable-react-component-boilerplate.git"
  },
  "author": "Lauren Eastridge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/formidable-react-component-boilerplate/issues"
  },
  "homepage": "https://github.com/FormidableLabs/formidable-react-component-boilerplate",
  "scripts": {
    "prepublish": "npm run check && npm run build",
    "clean-dist": "rimraf dist",
    "build-dist-min": "webpack --config=webpack.config.js",
    "build-dist-dev": "webpack --config=webpack.config.dev.js",
    "build-dist": "npm run clean-dist && npm run build-dist-min && npm run build-dist-dev",
    "clean-lib": "rimraf lib",
    "build-lib": "npm run clean-lib && babel --stage 0 src -d lib",
    "build": "npm run build-lib && npm run build-dist",
    "dev": "webpack-dev-server --config demo/webpack.config.dev.js --content-base demo",
    "hot": "webpack-dev-server --config demo/webpack.config.hot.js --hot --content-base demo",
    "open-demo": "opener http://localhost:8080/webpack-dev-server/",
    "demo-dev": "npm run open-demo & npm run dev",
    "demo-hot": "npm run open-demo & npm run hot",
    "demo": "npm run demo-hot",
    "lint-node": "eslint -c .eslintrc-node *.js demo/webpack.*.js",
    "lint-react": "eslint --ext .js,.jsx -c .eslintrc-react src demo/*.jsx",
    "lint": "npm run lint-node && npm run lint-react",
    "test": "echo 'TODO (1) Client 11, (2) Server 13, (3) Integration 12 tests.'",
    "check": "npm run lint && npm run test"
  },
  "dependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.3.2",
    "eslint": "^0.24.1",
    "rimraf": "^2.4.0",
    "webpack": "^1.10.0"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.0",
    "css-loader": "~0.9.0",
    "eslint-config-defaults": "^3.0.3",
    "eslint-plugin-react": "^2.6.4",
    "lodash": "^3.9.3",
    "opener": "^1.4.1",
    "react": "0.13.x",
    "react-hot-loader": "^1.2.8",
    "style-loader": "~0.8.0",
    "url-loader": "~0.5.5",
    "webpack-dev-server": "^1.10.0"
  }
}
