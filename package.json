{
  "name": "formidable-react-component-boilerplate",
  "version": "0.0.1",
  "description": "react component boilerplate",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/formidable-react-component-boilerplate.git"
  },
  "author": "Lauren Eastridge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/formidable-react-component-boilerplate/issues"
  },
  "homepage": "https://github.com/FormidableLabs/formidable-react-component-boilerplate",
  "scripts": {
    "postinstall": "npm run clean && npm run lib",
    "clean": "rimraf lib",
    "lib": "babel --stage 0 src/ -d lib/",
    "dist": "webpack && webpack --config=webpack.config.minified.js",
    "demo": "npm run dev-server & npm run open-demo",
    "dev-server": "webpack-dev-server --config demo/webpack.config.js --no-info --content-base demo/ --hot",
    "open-demo": "opener http://localhost:8080/webpack-dev-server/",
    "lint": "eslint --ext .js,.jsx src",
    "test": "karma start & npm run open-test",
    "open-test": "opener http://localhost:9876/"
  },
  "dependencies": {
  },
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "babel-eslint": "^3.1.0",
    "babel-loader": "^5.3.2",
    "chai": "^2.1.2",
    "css-loader": "~0.9.0",
    "eslint": "^0.24.0",
    "eslint-config-defaults": "^3.0.3",
    "eslint-loader": "^0.14.0",
    "eslint-plugin-react": "^2.6.4",
    "istanbul": "^0.3.13",
    "istanbul-instrumenter-loader": "^0.1.2",
    "karma": "~0.12.21",
    "karma-chrome-launcher": "~0.1.4",
    "karma-coverage": "^0.3.1",
    "karma-firefox-launcher": "~0.1.3",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "~0.1.3",
    "karma-script-launcher": "~0.1.0",
    "karma-sinon-chai": "^0.3.0",
    "karma-webpack": "^1.5.0",
    "lodash": "^3.9.3",
    "mocha": "^2.2.1",
    "opener": "^1.4.1",
    "react": "0.13.x",
    "rimraf": "^2.4.0",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "style-loader": "~0.8.0",
    "url-loader": "~0.5.5",
    "webpack": "^1.10.0",
    "webpack-dev-server": "^1.10.0"
  }
}

